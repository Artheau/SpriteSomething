      "F2:L","F2:R","F3:L","F3:R",
      "F4","F5","F6","F7"
    ],
    [
      "G0:L","G0:R","G1:L",
      "jumpLeft","jumpLeft1","G3:R",
      "G4:L",
      "G4:span",
      "G5:span",
