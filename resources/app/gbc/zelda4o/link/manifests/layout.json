{
  "$schema": "../../../../../schema/png-layout.schema.json",
  "comment": [
    "This file in some cases has a very specific ordering.  For instance, the first instance of any used_by list",
    "will also be the pose that is used to import the image.  For this reason, some poses will be out of order",
    "and this is intentional because a pose might be mirrored in its first occurance (grapple), or perhaps",
    "the way that it will be present in the ROM is different before and after modification (e.g. screw attack)"
  ],
  "border_color": [0, 0, 0, 255],
  "border_size": 0,
  "layout": [
    [
      "walkUp", "pushUp",
      "standLeft", "walkLeft",
      "fall","fall1","fall2",
      "carryDown","carryLeft"
    ],
    [
      "walkDown","standDown",
      "pushLeft","pushLeft1",
      "play","instrument","play1",
      "carryDown1",
      "squish",
      "dead"
    ],
    [
      "walkUp_S1","walkUp1_S1",
      "walkDown_S1","walkDown1_S1",
      "walkRight_S1","walkRight1_S1",
      "walkRight_S2","walkRight1_S2"
    ],
    [
      "blockUp","blockUp1",
      "blockDown_S1","blockDown1_S1",
      "blockLeft","blockLeft1",
      "blockRight","blockRight1"
    ]
  ],
  "images": {
    "walkUp": { "dimensions": [0,0,16,16], "grid": "A0" },
    "pushUp": { "parent": "walkUp", "grid": "A1" },
    "standLeft": { "parent": "walkUp", "grid": "A2" },
    "walkLeft": { "parent": "walkUp", "grid": "A3" },
    "fall": { "parent": "walkUp", "grid": "A4" },
    "fall1": { "dimensions": [0,0,8,16], "grid": "A5:left" },
    "fall2": { "parent": "fall1", "grid": "A5:right" },
    "carryDown": { "parent": "walkUp", "grid": "A6" },
    "carryLeft": { "parent": "walkUp", "grid": "A7" },

    "walkDown": { "parent": "walkUp", "grid": "B0" },
    "standDown": { "parent": "walkUp", "grid": "B1" },
    "pushLeft": { "parent": "walkUp", "grid": "B2" },
    "pushLeft1": { "parent": "pushLeft", "grid": "B3" },
    "play": { "parent": "walkUp", "grid": "B4" },
    "instrument": { "dimensions": [0,0,8,16], "grid": "B5:left" },
    "play1": { "parent": "play", "grid": "B5:right,B6:left" },
    "carryDown1": { "dimensions": [0,0,8,16], "grid": "B6:right" },
    "squish": { "dimensions": [0,0,8,16], "grid": "B7:left" },
    "dead": { "dimensions": [0,0,8,16], "grid": "B7:right" },

    "walkUp_S1": { "parent": "walkUp", "grid": "C0" },
    "walkUp1_S1": { "parent": "walkUp", "grid": "C1" },
    "walkDown_S1": { "parent": "walkUp", "grid": "C2" },
    "walkDown1_S1": { "parent": "walkUp", "grid": "C3" },
    "walkRight_S1": { "parent": "walkUp", "grid": "C4" },
    "walkRight1_S1": { "parent": "walkUp", "grid": "C5" },
    "walkRight_S2": { "parent": "walkUp", "grid": "C6" },
    "walkRight1_S2": { "parent": "walkUp", "grid": "C7" },

    "blockUp": { "parent": "walkUp", "grid": "D0" },
    "blockUp1": { "parent": "walkUp", "grid": "D1" },
    "blockDown_S1": { "parent": "walkUp", "grid": "D2" },
    "blockDown1_S1": { "parent": "walkUp", "grid": "D3" },
    "blockLeft": { "parent": "walkUp", "grid": "D4" },
    "blockLeft1": { "parent": "walkUp", "grid": "D5" },
    "blockRight": { "parent": "walkUp", "grid": "D6" },
    "blockRight1": { "parent": "walkUp", "grid": "D7" }
  }
}
