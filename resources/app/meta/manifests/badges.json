    "logo": "github",
    "url": "https://github.com/Artheau/SpriteSomething/tree/master"
  },
  {
    "title": "Stable Build Status",
    "keyword": "github/actions/workflow/status",
    "query": "ci.yml?branch=stable",
    "left": "stable build",
    "logo": "github",
    "url": "https://github.com/Artheau/SpriteSomething/tree/stable"
  },
  {
    "title": "Commits to Stable since last release",
    "keyword": "github/commits-since",
