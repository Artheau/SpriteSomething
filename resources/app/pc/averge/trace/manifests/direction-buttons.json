{
  "$schema": "../../../../../schema/spiffy-buttons.schema.json",
  "button-groups": [
    {
      "group-fish-key": "facing",
      "buttons": [
        {
          "fish-subkey": "left",
          "img": "arrow-left.png"
        },
        {
          "fish-subkey": "right",
          "img": "arrow-right.png",
          "default": true
        }
      ]
    },
    {
      "group-fish-key": "aiming",
      "buttons": [
        {
          "fish-subkey": "up",
          "img": "arrow-up.png"
        },
        {
          "fish-subkey": "diag_up",
          "img": "arrow-upright.png"
        },
        {
          "meta": "newline"
        },
        {
          "fish-subkey": "neutral",
          "img": "no-thing.png",
          "default": true
        },
        {
          "fish-subkey": "shoot",
          "img": "axiom-disruptor.png"
        },
        {
          "meta": "newline"
        },
        {
          "fish-subkey": "down",
          "img": "arrow-down.png"
        },
        {
          "fish-subkey": "diag_down",
          "img": "arrow-downright.png"
        }
      ]
    }
  ]
}
