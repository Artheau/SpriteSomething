{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "object",
	"title": "The Root Schema",
	"items": {
		"$id": "#/items",
		"type": "array",
		"title": "The Game Schema",
		"items": {
			"$id": "#/items/items",
			"type": "string",
			"title": "The Burn Schema",
			"default": "",
			"examples": [
				"ZSM",
				"Super Metroid",
				"Hyper Metroid"
			],
			"pattern": "^(.*)$"
		}
	}
}
