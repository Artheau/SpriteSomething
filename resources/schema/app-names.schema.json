{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://raw.githubusercontent.om/Artheau/SpriteSomething/master/schema/app-names.schema.json",
	"type": "object",
	"title": "SpriteSomething Application Names Schema",
	"required": [
		"noun",
		"pre",
		"post"
	],
	"properties": {
		"pre": {
			"$id": "#/properties/pre",
			"type": "array",
			"title": "The Pre Schema",
			"description": "A Prefix to the Application Name",
			"items": {
				"$id": "#/properties/pre/items",
				"type": "string",
				"title": "The Items Schema",
				"pattern": "^(.*)$"
			}
		},
		"noun": {
			"$id": "#/properties/noun",
			"type": "array",
			"title": "The Noun Schema",
			"description": "A Noun described by the Prefix to the Application Name",
			"items": {
				"$id": "#/properties/noun/items",
				"type": "string",
				"title": "The Items Schema",
				"pattern": "^(.*)$"
			}
		},
		"post": {
			"$id": "#/properties/post",
			"type": "array",
			"title": "The Post Schema",
			"description": "A Suffix to the Application Name",
			"items": {
				"$id": "#/properties/post/items",
				"type": "string",
				"title": "The Items Schema",
				"pattern": "^(.*)$"
			}
		}
	}
}
